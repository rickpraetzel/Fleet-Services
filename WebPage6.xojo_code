		Sub Action()
		  
		  if popupmenu1.text <> "Vehicle..." then
		    if PopupMenu3.text <> "Status..." then
		      SubmitTheChange
		    else
		      MsgBox "Please sppecify a vehicle..."
		    end if
		  else
		    MsgBox "Please sppecify a status..."
		  end if
		  
		  
		End Sub
