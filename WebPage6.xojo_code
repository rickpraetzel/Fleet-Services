	#tag Method, Flags = &h0
		Sub SendMessage(aServer as String, aMsg as EmailMessage, aUsername as String, aPassword as String, aPort as integer)
		  
		  // SMTP is port 25
		  
		  SMTPSecureSocket1.port = aport
		  // set the address of the server
		  SMTPSecureSocket1.address = aServer
		  // set username and password (only used for authenticated SMTP)
		  if EmailModule.smtpauthorizationtype = "None" then
		  else
		    SMTPSecureSocket1.username = aUsername
		    SMTPSecureSocket1.password = aPassword
		  end if
		  // add the message to the socket
		  SMTPSecureSocket1.messages.Append aMsg
		  // start the send process
		  SMTPSecureSocket1.SendMail
		  // show me
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SubmitTheChange()
		  if session.mysqldb <> nil then
		    if session.mysqldb.Connect then
		      
		      
		      dim rs as RecordSet
		      dim sqlstring as string
		      
		      sqlstring = "SELECT serial,status,notes from vehicles where serial = " + PopupMenu1.rowtag(PopupMenu1.ListIndex)
		      rs = session.mySQLdb.SQLSelect(sqlstring)
		      if rs <> nil then
		        if not rs.bof and not rs.eof then
		          rs.edit
		          rs.field("status").value = popupmenu3.text
		          rs.field("notes").value = ReplaceAll(TextArea1.text,"'","")
		          rs.update
		          if session.mysqldb.ErrorCode = 0 then
		            AutoDiscoveryClass1.SendMessageToGroup(1,"Hi")
		            '----- clear the contents of this page
		            ClearThePage
		            MsgBox "Thank you."
		            '----- return the app to webpage 0
		            webpage0.show
		            '-----
		            IF 1 = 2 THEN 'we are not sending text messages
		              if TextArea1.text <> "" then 'send SMS and emails to Dave,Bill,Rick
		                dim amsg as EmailMessage
		                dim body as string
		                dim pnum,recipient,s as string
		                dim i as integer
		                amsg = new EmailMessage
		                if ubound(recips) >= 0 then
		                  for i = 0 to ubound(recips)
		                    s = recips(i).name
		                    amsg.AddRecipient recips(i).name
		                    s = recips(i).sms
		                    pnum = recips(i).sms
		                    if pnum <> "" then
		                      pnum = ReplaceAll(pnum,"-","")
		                      pnum = replaceall(pnum,".","")
		                      if recips(i).carrier = "Verizon" then
		                        recipient = pnum + "@vtext.com"
		                      elseif recips(i).carrier = "T-Mobile" then
		                        recipient = pnum+ "@tmomail.net"
		                      elseif recips(i).carrier = "Virgin" then
		                        recipient = pnum + "@vmobl.com"
		                      elseif recips(i).carrier = "Cingular" then
		                        recipient = pnum + "@cingularme.com"
		                      elseif recips(i).carrier = "Sprint" then
		                        recipient = pnum + "@messaging.sprintpcs.com"
		                      elseif recips(i).carrier = "Nextel" then
		                        recipient = pnum + "@messaging.nextel.com"
		                      end if
		                      amsg.AddRecipient recipient
		                    end if
		                  next
		                  amsg.FromAddress = "FleetServices@zionadventures.com"
		                  amsg.Subject = "Vehicle Status Change"
		                  body = "From: " + PopupMenu2.text
		                  body = body + EndOfLine + "Vehicle: " + popupmenu1.text
		                  body = body + EndOfLine + "Sent from the Transportation iPad."
		                  body = body + EndOfLine + "Status is changed to: " + popupmenu3.text + "."
		                  body = body + EndOfLine + TextArea1.text
		                  amsg.BodyPlainText = body
		                  SendMessage(EmailModule.smtphost,amsg,EmailModule.smtpusername,EmailModule.smtppassword,EmailModule.smtpport)
		                else
		                  
		                end if
		              else
		              end if
		            else
		            end if '-----
		          end if
		        end if
		      end if
		      
		    end if
		  end if
		  
		End Sub
	#tag EndMethod


